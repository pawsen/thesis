@Article{kerschen2009b,
  Title                    = {Nonlinear normal modes, Part II: Toward a practical computation using numerical continuation techniques},
  Author                   = {Peeters, M. and Viguie, R. and Serandour, G. and Kerschen, G. and Golinval, J. -C.},
  Journal                  = {Mechanical Systems and Signal Processing},
  Year                     = {2009},
  Number                   = {1},
  Pages                    = {195-216},
  Volume                   = {23},

  Abstract                 = {The concept of nonlinear normal modes (NNMs) is discussed in the present paper and its companion, Part I. One reason of the still limited use of NNMs in structural dynamics is that their computation is often regarded as impractical. However, when resorting to numerical algorithms, we show that the NNM computation is possible with limited implementation effort, which paves the way to a practical method for determining the NNMs of nonlinear mechanical systems. The proposed algorithm relies on two main techniques, namely a shooting procedure and a method for the continuation of NNM motions. The algorithm is demonstrated using four different mechanical systems, a weakly and a strongly nonlinear two-degree-of-freedom system, a simplified discrete model of a nonlinear bladed disk and a nonlinear cantilever beam discretized by the finite element method. (C) 2008 Elsevier Ltd. All rights reserved},
  Doi                      = {10.1016/j.ymssp.2008.04.003},
  File                     = {:/home/paw/ownCloud/speciale/litteratur/artikler/Kerschen(2008b)-NonlinearNormalModes-II.pdf:PDF},
