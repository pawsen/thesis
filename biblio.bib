@Article{shaw1993a,
  Title                    = {Normal modes for non-linear vibratory systems},
  Author                   = {Shaw, S. W. and Pierre, C.},
  Journal                  = {Journal of Sound and Vibration},
  Year                     = {1993},
  Number                   = {1},
  Pages                    = {85-124, 85-124},
  Volume                   = {164},

  Abstract                 = {A methodology is presented which extends to non-linear systems the concept of normal modes of motion which is well developed for linear systems. The method is constructive for weakly non-linear systems and provides the physical nature of the normal modes along with the non-linear differential equations which govern their dynamics. It also provides the non-linear co-ordinate transformation which relates the original system co-ordinates to the modal co-ordinates. Using this transformation, the authors demonstrate how an approximate non-linear version of superposition can be employed to reconstruct the overall motion from the individual non-linear modal dynamics. The results presented herein for non-linear systems reduce to modal analysis for the linearized system when non-linearities are neglected, even though the approach is entirely different from the traditional one. The tools employed are from the theory of invariant manifolds for dynamical systems and were inspired by the center manifold reduction technique. The basic ideas are outlined, a few examples are presented and some natural extensions and applications of the method are briefly described in the conclusions.},
  Doi                      = {10.1006/jsvi.1993.1198},
  File                     = {:/home/paw/ownCloud/speciale/litteratur/artikler/shawn(1993)-NormalModes.pdf:PDF},
  ISSN                     = {10958568, 0022460x},
  Language                 = {eng}
}

