@Article{Lee200541,
  Title                    = {Complicated dynamics of a linear oscillator with a light, essentially nonlinear attachment },
  Author                   = {Young Sup Lee and Gaetan Kerschen and Alexander F. Vakakis and Panagiotis Panagopoulos and Lawrence Bergman and D. Michael McFarland},
  Journal                  = {Physica D: Nonlinear Phenomena },
  Year                     = {2005},
  Number                   = {1–2},
  Pages                    = {41 - 69},
  Volume                   = {204},

  Abstract                 = {We study the dynamics of a two-degree-of-freedom (DOF) nonlinear system consisting of a grounded linear oscillator coupled to a light mass by means of an essentially nonlinear (nonlinearizable) stiffness. We consider first the undamped system and perform a numerical study based on non-smooth transformations to determine its periodic solutions in a frequency–energy plot. It is found that there is a sequence of periodic solutions bifurcating or emanating from the main backbone curve of the plot. We then study analytically the periodic orbits of the undamped system using a complexification/averaging technique in order to determine the frequency contents of the various branches of solutions, and to understand the types of oscillation performed by the system at the different regimes of the motion. The transient responses of the weakly damped system are then examined, and numerical wavelet transforms are used to study the time evolutions of their harmonic components. We show that the structure of periodic orbits of the undamped system greatly influences the damped dynamics, as it causes complicated transitions between modes in the damped transient motion. In addition, there is the possibility of strong passive energy transfer (energy pumping) from the linear oscillator to the nonlinear attachment if certain periodic orbits of the undamped dynamics are excited by the initial conditions. },
  Doi                      = {http://dx.doi.org/10.1016/j.physd.2005.03.014},
  File                     = {:/home/paw/ownCloud/speciale/litteratur/artikler/Lee(2005)-ComplicatedDynamicsOscillatorNonlinarAttachment.pdf:PDF},
  ISSN                     = {0167-2789},
  Keywords                 = {Resonance capture},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S016727890500120X}
}

